{
  "manifest_version": 3,
  "name": "Ad Blocker",
  "version": "1.0",
  "description": "Chrome extension to block ads.",
  "permissions": ["storage", "declarativeNetRequest", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon48.png"
  }
}
